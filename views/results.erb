<div id="wrapper">

        <!-- Sidebar -->
        <div id="sidebar-wrapper">
            <ul class="sidebar-nav">
                <li class="sidebar-brand">
                    <a href="/">
                        Beer me a new search
                    </a>
                </li>
            </ul>

            <ul class="location-card center-block">
              <% @venues.each do |t| %>
               <li>
                    <img class="img-responsive pic" src="<%= t.photo_url %>" />
                    <div class="bio">
                        <h2 class="name"><%= t.name %></h2>
                        <hr>
                        <h3 class="title"><%= t.primary_category %></h3>
                        <h3 class="dist">1.1 miles</h3>
                        <p class="desc">Co-Founder and CTO of Untappd, Web Developer, Beer Drinker &amp; Community Guy</p>
                        <a href="#" class="btn btn-danger more_detatils_btn">View Details</a>
                    </div>
                </li>
              <% end %>
             </ul>

        </div>
        <!-- /#sidebar-wrapper -->

        <!-- Page Content -->
        <div id="page-content-wrapper">
          <div id="map2"></div>
                  <!-- <a href="#menu-toggle" class="btn btn-default" id="menu-toggle">Toggle Menu</a> -->
        </div>
        <!-- /#page-content-wrapper -->
    </div>
    <!-- /#wrapper -->


    <script src="<%= url('/javascript/js/results_map.js') %>"></script>
